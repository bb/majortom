<?xml version="1.0" encoding="UTF-8"?>
<jli-mapping
	xmlns="http://www.topicmapslab.de/mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.topicmapslab.de/mapping mapping.xsd">

	<connection-settings>
		<url>hass.tm.informatik.uni-leipzig.de:3306</url>
		<name>jli_test</name>
		<user>root</user>
		<password></password>
		<catalog>public</catalog>
	</connection-settings>

	<mapping>
		<!--  testGetAssociationsPlayed() -->			
		<topic id="testGetAssociationsPlayed">
			<identifier type="SI">
				<jdbc_link table="testGetAssociationsPlayed" column="si"/>
			</identifier>
		</topic>
		<association type="http://ass/testGetAssociationsPlayed" table="testGetAssociationsPlayed">
			<role topicId="testGetAssociationsPlayed" type="http://role1/testGetAssociationsPlayed">
				<jdbc_link column="id"/>
			</role>
			<role topicId="testGetAssociationsPlayed" type="http://role2/testGetAssociationsPlayed">
				<jdbc_link column="id2"/>
			</role>
		</association>
		
		
	</mapping>
	
</jli-mapping>
